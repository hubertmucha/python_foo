def backslash():
    """
    The function prints one line on the screen:
    
    This is backslash: "\"
    
    - no empty lines
    - no white characters at the beginning
    - no white characters at the end
    - single space between the words
    - single end-of-line character at the end of the string

    Returns
    -------
    0

    """
    print("This is backslash: \"\\\"")
    return 0

backslash()
